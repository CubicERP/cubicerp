<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		
		<record model='ir.ui.view' id='view_location_form'>
			<field name='name'>stock.location.form.inherit.pe</field>
			<field name='model'>stock.location</field>
			<field name='inherit_id' ref='stock.view_location_form'/>
			<field name="priority">90</field>
			<field name='arch' type='xml'>
				<xpath expr="//group[@string='Localization']" position="after">
					<group name="l10n_pe" string="Peruvian Localization Settings" invisible="1">
					</group>
				</xpath>
			</field>
		</record>
		
	</data>
</openerp>
